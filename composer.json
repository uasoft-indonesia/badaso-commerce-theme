{
  "version": "0.3.4",
  "name": "badaso/commerce-theme",
  "description": "Zero development for commerce theme on badaso",
  "keywords": [
    "laravel",
    "admin",
    "panel",
    "dashboard",
    "pwa",
    "vue",
    "generator",
    "blog",
    "badaso",
    "commerce"
  ],
  "license": "proprietary",
  "homepage": "https://badaso.uatech.co.id/",
  "support": {
    "issues": "https://github.com/uasoft-indonesia/badaso-commerce-theme/issues",
    "discussion": "https://github.com/uasoft-indonesia/badaso-commerce-theme/discussions",
    "source": "https://github.com/uasoft-indonesia/badaso-commerce-theme"
  },
  "type": "library",
  "require": {
    "badaso/core": "^2.0",
    "badaso/content-module": "^2.0",
    "badaso/commerce-module": "^0.3.0",
    "inertiajs/inertia-laravel": "^0.4.5",
    "tightenco/ziggy": "^1.4"
  },
  "authors": [
    {
      "name": "UASOFT",
      "email": "hello@uatech.co.id"
    }
  ],
  "minimum-stability": "alpha",
  "autoload": {
    "psr-4": {
      "Uasoft\\Badaso\\Theme\\CommerceTheme\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Uasoft\\Badaso\\Theme\\CommerceTheme\\Providers\\CommerceThemeProvider"
      ]
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^1.4"
  }
